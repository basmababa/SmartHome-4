<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ffb1b1b1" android:endColor="#ffc3c3c3" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffdadada" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ff40b805" android:endColor="#ff40b805" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff19e000" />
            </shape>
        </clip>
    </item>
</layer-list>
